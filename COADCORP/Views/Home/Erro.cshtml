@using Coad.GenericCrud.Extensions
@{
    Layout = null;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" dir="ltr" lang="ro-RO">
<head profile="http://gmpg.org/xfn/11">
    <title>COADCORP </title>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=8" />
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <script src="@Url.Content("~/Scripts/jquery-1.11.1.min.js")" type="text/javascript"></script>
    <script type="text/javascript" src="js/jquery-global.js"></script>
    <link href="~/Content/themes/PortalCoad/css/backgrounds.css" rel="stylesheet" />
    <link href="~/Content/themes/PortalCoad/css/style.css" rel="stylesheet" />
    
    @Styles.Render("~/Content/themes/base/default.css")
    @Styles.Render("~/Content/themes/base/bootstrap.css")    
    @Styles.Render("~/Content/themes/base/coadcorp-boostrap.css")
    
    @Scripts.Render("~/Scripts/bootstrap.min.js")
    <!--[if IE]>
     <script type="text/javascript" src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <script type="text/javascript">
        $(function () {
            $('[data-toggle="popover"]').popover({
                html: true,
                placement: "auto"
            });
        })
    </script>
</head>
<body>
    <div class="wrapper">
        <div class="mainWrapper">
            <div class="leftHolder">
             	<a href="http://www.coad.com.br" title="COAD" class="logo">COAD</a>
            
               <div class="panel panel-corp">
                <div class="panel-heading erro-head"><span class="glyphicon glyphicon-warning-sign"></span> @Html.ExceptionMessage("exceptionMsg")</div>
      
                    <div class="panel-body">
                        <ul class="select2-ul" style="max-height: 212px; overflow-y: auto; position: initial; font-size: 15px;">                        
                            @Html.ExceptionMessage("exceptionMsg", true)
                        </ul>                
                    </div>     
                
            </div>
          </div>
            <div class="rightHolder">
                <br />
                <br />
                  <br />
                <br />
                <div class="message">
                   
                </div>
                <div class="warning">
                    <img src="/images/Bloqued_256.png" title="Oooops.... ocorreu algum problema ao tentar acessar a pagina desejada." id="warning" style="height: 120px; width: 120px;" />
                </div>
                
                <div class="tryToMessage">
                    <ul>
                        <li>Visite nosso site <a href="http://www.coad.com.br/" title="Home"></a></li>
                    </ul>
                    </div>
            </div>
            <footer>
                <p class="copy">Copyright © @DateTime.Now.Year All rights reserved. </p>
                <menu>
                    <li><a href="http://www.coad.com.br/" title="Home">Home</a></li>
                    <li><a href="http://www.coad.com.br/home/boas_vindas" title="O que mudou">O que mudou</a></li>
                    <li><a href="http://www.coad.com.br/cursos" title="Services">Cursos</a></li>
                    <li><a href="http://www.coad.com.br/home/produtos" title="Produtos">Produtos</a></li>
                    <li class="last"><a href="http://www.coad.com.br/contato" title="Contato">Contato </a></li>
                </menu>
            </footer>
            <!-- end footer -->
        </div>
    </div>
    <!-- end .wrapper -->
</body>
</html>
